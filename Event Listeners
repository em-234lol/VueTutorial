<script type="module">
import { createApp } from 'vue'

createApp({
  data() {
    return {
      count: 0
    }
  },
  methods:{  
 ///Note:  methodes it is a reseved word (" diff than methode")
    increment(){
      
      this.count++
    }
  }
  
}).mount('#app')
</script>

<div id="app">
  <!-- make this button work -->
  <button @click='increment'> {{ count }}</button>
</div>
